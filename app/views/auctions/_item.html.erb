<tbody>
<dl>  
  <% @auctions.each do |store| %>
  <tr>
    <dt>name</dt>
    <dd><%= store.item_name %></dd>
    <dt>price</dt>
    <dd><%= store.price %></dd>
  </tr>
  <% if store.editable_by?(current_user) %>
  <%= link_to "Edit", edit_auction_path(store.id), class: "btn btn-default" %>
  <%= link_to "Delete", auction_path(store.id), method: :delete, class: "btn btn-default" %>
  <% end %>
  <% end %>
</dl>
</tbody>
