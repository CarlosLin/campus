  <div class="row">
  <tr>
    <dt>name</dt>
    <dd><%= @auction.item_name %></dd>
    <dt>price</dt>
    <dd><%= @auction.price %></dd>
    <dt>quantity</dt>
    <dd><%= @auction.quantity%></dd>
  </tr>
  </div>
  <div class="row">
    <ul class="thumbnails files">
    <% @auction.photos.each do |p| %>
    <%= image_tag p.image.url %>
    <% end %>
    </ul>
  </div>
  <%= link_to (auctions_path) do %>
  <button type="button" class="btn btn-primary">
    商品列表
  </button>
  <% end %>
